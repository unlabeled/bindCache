{
  "name": "bindcache",
  "version": "1.0.0",
  "description": "Cache/memoize result of bind function with its context or/and args (with shims)",
  "main": "build/index.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "start": "webpack --mode development --watch",
    "build": "webpack --mode production"
  },
  "typings": "bindcache.d.ts",
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/unlabeled/bindCache.git"
  },
  "keywords": [
    "cache",
    "momoize",
    "bind"
  ],
  "author": {
    "name": "Timofey Tkachenko",
    "email": "ttimatika@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unlabeled/bindCache/issues"
  },
  "homepage": "https://github.com/unlabeled/bindCache#readme",
  "dependencies": {
    "cachebind": "^1.0.0"
  },
  "devDependencies": {
    "lint-staged": "^7.0.4",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "peerDependencies": {
    "es6-map": "^0.1.5",
    "es6-weak-map": "^2"
  }
}
